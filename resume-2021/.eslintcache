ate(null);\r\n  // store the filter keyword in another state\r\n  const [filterKey, setFilterKey] = useState(\"*\");\r\n  const [activebutton, setActivebutton] = useState(\"all\");\r\n\r\n  // Pagination States\r\n  const [pageNumber, setPageNumber] = useState(0);\r\n  const usersPerPage = 6;\r\n  const pagesVisited = pageNumber * usersPerPage;\r\n  // Looping datacollection for lenght of data\r\n  const pagecount = Object.entries(portfolioGallery).map((key) => key[1]);\r\n  const title = pagecount.map((title) => title.length);\r\n  const totalblogs = title.reduce((a, b) => a + b, 0);\r\n  const pageCount = Math.ceil(totalblogs / usersPerPage);\r\n\r\n  // Pagination filtering\r\n  const slicingAllPages = () => {\r\n    const elements = Array.from(galleryContainer.current.children);\r\n    // const elementArray = [...elements];\r\n    let slicing = elements\r\n      .slice(pagesVisited, pagesVisited + usersPerPage)\r\n      .map((item) => item.classList[1]);\r\n    return slicing.map((slice) => `.${slice}`);\r\n  };\r\n  // initialize an Isotope object with configs\r\n  useEffect(() => {\r\n    // setHeight(isOpen ? \"0px\" : `${bodyContent.current.scrollHeight}px`);\r\n    if (portfolioIsOpen) {\r\n      setIsotope(\r\n        new Isotope(\".gallery__container\", {\r\n          itemSelector: \".filter-item\",\r\n          layoutMode: \"fitRows\",\r\n          stagger: 40,\r\n          transitionDuration: \"0.8s\",\r\n          hiddenStyle: {\r\n            opacity: 0,\r\n            transform: \"scale(0.001)\",\r\n          },\r\n          visibleStyle: {\r\n            opacity: 1,\r\n            transform: \"scale(1)\",\r\n          },\r\n        })\r\n      );\r\n    }\r\n  }, [portfolioIsOpen]);\r\n\r\n  useEffect(() => {\r\n    if (isotope) {\r\n      filterKey === \"*\"\r\n        ? isotope.arrange({\r\n            filter: slicingAllPages(),\r\n          })\r\n        : isotope.arrange({ filter: `.${filterKey}` });\r\n    }\r\n  }, [filterKey, pageNumber]);\r\n\r\n  const handleClickButton = (e) => {\r\n    const value = e.currentTarget.attributes.value.value;\r\n    setActivebutton(value);\r\n    setFilterKey(value === \"all\" ? \"*\" : value);\r\n  };\r\n\r\n  const toggleActiveButton = (button) => {\r\n    if (activebutton === button) {\r\n      return \"shape active\";\r\n    } else {\r\n      return \"shape\";\r\n    }\r\n  };\r\n\r\n  const changePage = ({ selected }) => {\r\n    setPageNumber(selected);\r\n  };\r\n\r\n  const options = {\r\n    buttons: {\r\n      backgroundColor: 'rgba(30,30,36,0.8)',\r\n      iconColor: 'rgba(255, 255, 255, 0.8)',\r\n      iconPadding: '10px',\r\n      showAutoplayButton: false,\r\n      showCloseButton: true,\r\n      showDownloadButton: false,\r\n      showFullscreenButton: false,\r\n      showNextButton: true,\r\n      showPrevButton: true,\r\n      showThumbnailsButton: true,\r\n      size: '40px'\r\n    }\r\n}\r\n\r\n  return (\r\n    <div className=\"profileBody\">\r\n      <div className=\"buttons\">\r\n        <ul>\r\n          {portfolioButtons.map((button, index) => (\r\n            <li key={index} value={button} onClick={handleClickButton}>\r\n              <PortfolioButtons\r\n                buttonName={button}\r\n                activeButton={toggleActiveButton}\r\n              />\r\n            </li>\r\n          ))}\r\n        </ul>\r\n      </div>\r\n      <SRLWrapper options={options}>\r\n        <div\r\n          ref={galleryContainer}\r\n          className=\"gallery__container\"\r\n          style={{ maxHeight: \"370px\", transition: \"height 0.6s ease\" }}\r\n        >\r\n          {Object.entries(portfolioGallery).map((title, index) => (\r\n            <Gallery\r\n              key={title[0] + index}\r\n              classname={`filter-item ${title[0]}`}\r\n              imgs={title[1]}              \r\n            />\r\n          ))}\r\n        </div>\r\n      </SRLWrapper>\r\n\r\n      {/************************************************************* Pagination */}\r\n      {activebutton === \"all\" && (\r\n        <div className=\"portfolio__navigator\">\r\n          <ReactPaginate\r\n            previousLabel=\"Prev\"\r\n            nextLabel=\"Next\"\r\n            pageCount={pageCount}\r\n            onPageChange={changePage}\r\n            containerClassName=\"paginationBttns\"\r\n            // previousClassName=\"previousBttn\"\r\n            // nextLinkClassName=\"nextBttn\"\r\n            disabledClassName=\"paginationDisabled\"\r\n            activeClassName=\"paginationActive\"\r\n          />\r\n\r\n          <div className=\"page__number\">\r\n            Page: {pageNumber + 1} of {pageCount}\r\n          </div>\r\n        </div>\r\n      )}\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default PortfolioBody;\r\n","/Volumes/Sanjay Shre/E/Personal Record/Sanjay2021-Resume/resume-2021/src/Components/Accordion/Accordion.js",[],"/Volumes/Sanjay Shre/E/Personal Record/Sanjay2021-Resume/resume-2021/src/Components/Resume/Employments.js",[],"/Volumes/Sanjay Shre/E/Personal Record/Sanjay2021-Resume/resume-2021/src/Components/Resume/Skills.js",[],"/Volumes/Sanjay Shre/E/Personal Record/Sanjay2021-Resume/resume-2021/src/Components/Portfolio/PortfolioDatas.js",[],"/Volumes/Sanjay Shre/E/Personal Record/Sanjay2021-Resume/resume-2021/src/Components/Portfolio/PortfolioButtons.js",[],"/Volumes/Sanjay Shre/E/Personal Record/Sanjay2021-Resume/resume-2021/src/Components/Portfolio/Gallery.js",[],"/Volumes/Sanjay Shre/E/Personal Record/Sanjay2021-Resume/resume-2021/src/Components/Accordion/Chevron.js",[],"/Volumes/Sanjay Shre/E/Personal Record/Sanjay2021-Resume/resume-2021/src/Components/Resume/employmentDatas.js",["109"],"/Volumes/Sanjay Shre/E/Personal Record/Sanjay2021-Resume/resume-2021/src/Components/Resume/SkillsData.js",[],"/Volumes/Sanjay Shre/E/Personal Record/Sanjay2021-Resume/resume-2021/src/Components/Resume/Designs.js",[],"/Volumes/Sanjay Shre/E/Personal Record/Sanjay2021-Resume/resume-2021/src/Components/Resume/CircularProgress.js",["110"],"import React, { useEffect, useRef, useState } from \"react\";\r\n\r\nfunction CircularProgress({ bgcolor, completed }) {\r\n  const [offset, setOffset] = useState(132);\r\n  const circleRef = useRef(null);\r\n\r\n  const size = 40;\r\n  const progress = completed;\r\n  const stokeWidth = 8;\r\n  const circleOneColor = \"rgb(224, 224, 222)\";\r\n  const circleTwoColor = bgcolor;\r\n\r\n  const center = size / 2;\r\n  const radius = (size - stokeWidth) / 2;\r\n  const circumference = 2 * Math.PI * radius;\r\n\r\n  useEffect(() => {\r\n    setInterval(() => {\r\n      const progressOffset = ((100 - progress) / 100) * circumference;\r\n      setOffset(progressOffset);\r\n    }, 1500);\r\n    circleRef.current.style = \"transition: stroke-dashoffset 2.5s ease-in-out\";\r\n    clearInterval();    \r\n  }, [setOffset]);\r\n\r\n  return (\r\n    <div className=\"circularProgress__container\">\r\n      <svg width={size} height={size} className=\"circularprogress\" rotate=\"-90\" origin={`${radius/2}, ${radius/2}`} >\r\n        <circle\r\n          className=\"circle-bg\"\r\n          stroke={circleOneColor}\r\n          cx={center}\r\n          cy={center}\r\n          r={radius}\r\n          strokeWidth={stokeWidth - 2}\r\n        ></circle>\r\n        <circle\r\n          className=\"circle\"\r\n          ref={circleRef}\r\n          stroke={circleTwoColor}\r\n          cx={center}\r\n          cy={center}\r\n          r={radius}\r\n          strokeWidth={stokeWidth}\r\n          strokeDasharray={circumference}\r\n          strokeDashoffset={offset}\r\n          // strokeLinecap=\"round\"\r\n        ></circle>\r\n        <text x={center} y={center + 5} className=\"percentage\">\r\n          {progress}%\r\n        </text>\r\n      </svg>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default CircularProgress;\r\n","/Volumes/Sanjay Shre/E/Personal Record/Sanjay2021-Resume/resume-2021/src/Components/Resume/ProgressBar/ProgressBar.js",["111"],"import React, { useEffect, useState } from \"react\";\r\nimport { useStateValue } from \"../../../StateManagement/StateProvider\";\r\n\r\nfunction ProgressBar({ bgcolor, completed, title }) {\r\n  const [animate, setAnimate] = useState(0);\r\n  const [isOpen, setIsOpen] = useState(false);\r\n  const [{ resumeIsOpen }] = useStateValue();\r\n\r\n  useEffect(() => {\r\n    resumeIsOpen ? setIsOpen(true) : setIsOpen(false)    \r\n  }, [resumeIsOpen, setIsOpen]);\r\n\r\n  useEffect(() => {\r\n    isOpen ? \r\n    setInterval(() => {\r\n      setAnimate(completed)\r\n    }, 1000) :\r\n    setAnimate(0)\r\n    clearInterval()\r\n  }, [isOpen, setAnimate]);\r\n\r\n  const containerStyles = {\r\n    height: 15,\r\n    width: \"100%\",\r\n    backgroundColor: \"#e0e0de\",\r\n    borderRadius: 5,\r\n    marginTop: 3,\r\n    marginBottom: 8,\r\n  };\r\n  const fillerStyles = {\r\n    height: \"100%\",\r\n    width: `${animate}%`,\r\n    backgroundColor: bgcolor,\r\n    borderRadius: \"inherit\",\r\n    textAlign: \"right\",\r\n    transition: \"width 1s ease-in-out\",\r\n  };\r\n\r\n  const labelStyles = {\r\n    marginRight: 10,\r\n    color: \"white\",\r\n    fontWeight: \"bold\",\r\n    fontSize: 12,\r\n  };\r\n\r\n  const labletitle = {\r\n    fontSize: 12,\r\n    fontWeight: 500,\r\n    color: \"#646464\",\r\n    paddingBottom: 2,\r\n  };\r\n\r\n  return (\r\n    <div className=\"progressbar\">\r\n      <h4 style={labletitle}>{title}</h4>\r\n      <div style={containerStyles}>\r\n        <div style={fillerStyles}>\r\n          <p style={labelStyles}>{`${animate}%`}</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default ProgressBar;\r\n","/Volumes/Sanjay Shre/E/Personal Record/Sanjay2021-Resume/resume-2021/src/Components/Profile/ProfileHeader.js",[],{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","severity":1,"message":"117","line":72,"column":6,"nodeType":"118","endLine":72,"endColumn":29,"suggestions":"119"},{"ru